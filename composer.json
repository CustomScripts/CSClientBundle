{
    "name": "customscripts/client-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to manage clients and contacts",
    "keywords": [
        "clients",
        "contacts"
    ],
    "homepage": "http://customscripts.co.za",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "info@customscripts.co.za"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "customscripts/core-bundle": "dev-master",
        "apy/datagrid-bundle" : "@stable"
    },
    "suggest" : {
        "doctrine/doctrine-fixtures-bundle" : "Load fixtures for bundle"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "CS": "src/"
        }
    }
}
